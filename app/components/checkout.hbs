<table class="w-full text-sm text-left rtl:text-right border border-gray-100 text-gray-500 dark:text-gray-400">
  {{#each @data as |result|}}
  <tr class="odd:bg-white odd:dark:bg-gray-900 even:bg-gray-50 even:dark:bg-gray-800 border-b dark:border-gray-700">
    <td class="px-6 py-4">{{result.name}}</td>
    <td class="px-6 py-4">({{result.price}})</td>
    <td class="px-6 py-4">{{result.total}}</td>
  </tr>
  {{/each}}
</table>